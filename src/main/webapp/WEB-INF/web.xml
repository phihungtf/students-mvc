  </servlet>
  <servlet-mapping>
    <servlet-name>CourseControllerServlet</servlet-name>
    <url-pattern>/course</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ScoreControllerServlet</display-name>
    <servlet-name>ScoreControllerServlet</servlet-name>
